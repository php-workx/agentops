{
  "name": "knowledge-os-marketplace",
  "version": "2.0.0",
  "schema": "anthropic-2025-marketplace-v1",
  "owner": {
    "name": "Brandon Fuller",
    "email": "fullerbt@users.noreply.github.com"
  },
  "metadata": {
    "description": "Knowledge OS Marketplace - Modular Claude Code extensions with 3-tier architecture",
    "pluginRoot": ".."
  },
  "tiers": {
    "foundation": {
      "description": "Required core plugins - install first",
      "plugins": ["foundation-core", "foundation-bundles", "foundation-quality", "foundation-learning", "foundation-meta"]
    },
    "domain": {
      "description": "Role-based plugins - pick 1-3 based on your work",
      "plugins": ["domain-software-engineer", "domain-devops", "domain-platform", "domain-data", "domain-security", "domain-documentation", "domain-mobile", "domain-innovation"]
    },
    "specialist": {
      "description": "Deep expertise plugins - optional, install as needed",
      "plugins": ["specialist-golang", "specialist-python", "specialist-typescript", "specialist-shell", "specialist-rust", "specialist-java", "specialist-performance", "specialist-accessibility"]
    },
    "niche": {
      "description": "Specialized domains - install for specific use cases",
      "plugins": []
    }
  },
  "installation_profiles": {
    "minimal": {
      "description": "Core workflow only",
      "plugins": ["foundation-core", "foundation-bundles"]
    },
    "developer": {
      "description": "Full-stack developer setup",
      "plugins": ["foundation-core", "foundation-bundles", "foundation-quality", "foundation-learning", "foundation-meta", "domain-software-engineer", "specialist-typescript", "specialist-python"]
    },
    "devops": {
      "description": "DevOps/Platform engineer setup",
      "plugins": ["foundation-core", "foundation-bundles", "foundation-quality", "foundation-learning", "foundation-meta", "domain-devops", "domain-platform", "specialist-shell"]
    },
    "data": {
      "description": "Data engineer/scientist setup",
      "plugins": ["foundation-core", "foundation-bundles", "foundation-quality", "foundation-learning", "foundation-meta", "domain-data", "specialist-python"]
    },
    "full": {
      "description": "Everything",
      "plugins": ["*"]
    }
  },
